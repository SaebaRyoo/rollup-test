  "description": "a picture wartermark for react",
  "main": "dist/react-picture-watermark.js",
  "module": "dist/react-picture-watermark.es.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "jest -u --color --config=./jest.config.js",
    "build": "rollup -c",
